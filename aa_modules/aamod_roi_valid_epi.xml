<?xml version="1.0" encoding="utf-8"?>
<aap>
    <tasklist>
        <currenttask mfile_alias='aamod_roi_valid' domain='study' desc='Find valid ROIs for EPI' modality='MRI'>
            <AbsVoxThr>10</AbsVoxThr>
            <SubjRemoveStat options='mode|median|mean|none' ui='optionlist'>mode</SubjRemoveStat>
            <SubjRemoveThr></SubjRemoveThr>
            <ROIRemoveThr>0</ROIRemoveThr>

            <inputstreams>
               <stream isrenameable='1'>roidata_epi</stream>
            </inputstreams>
            
            <outputstreams>
            	<stream isrenameable='1'>valid_roi_epi</stream>
            </outputstreams>
			            
        </currenttask>
    </tasklist>
</aap>

