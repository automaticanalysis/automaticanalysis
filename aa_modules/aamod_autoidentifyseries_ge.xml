<?xml version="1.0" encoding="utf-8"?>
<aap>
    <tasklist>
        <currenttask mfile_alias='aamod_autoidentifyseries' domain='subject' desc='Automatically identify series numbers' modality='MRI'>
            
            <qsub>
                <timeBase>0.5</timeBase>
                <memoryBase>0.5</memoryBase>
            </qsub>
            
            <permanenceofoutput>1</permanenceofoutput>
            
            <!-- This is typically SeriesDescription for GE, and ProtocolName for Siemens -->
            
            <dicom_protocol_field>SeriesDescription</dicom_protocol_field>
            <orderdicomsby>InstanceNumber</orderdicomsby>
            <outputstreams>
                <stream>autoidentifyseries</stream>
            </outputstreams>
            
        </currenttask>
    </tasklist>
</aap>
