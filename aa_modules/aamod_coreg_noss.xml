<?xml version="1.0" encoding="utf-8"?>
<aap>
    <tasklist>
        <currenttask domain='subject' desc='SPM coregistration' modality='MRI'>
            
            <qsub>
                <timeBase>0.05</timeBase>
                <memoryBase>0.2</memoryBase>
            </qsub>
            
            <permanenceofoutput>1</permanenceofoutput>
            
            <!-- NB. The order of the streams is important; the first stream
            name is used as the input stream (e.g., 'structural' or 't1'). -->
            
            <inputstreams>
                <stream>structural</stream>
                <stream>meanepi</stream>                    
            </inputstreams>
            
            <outputstreams>
                <stream>structural</stream>
            </outputstreams>
            
        </currenttask>
    </tasklist>
</aap>

